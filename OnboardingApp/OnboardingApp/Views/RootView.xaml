<hc:Window x:Class="OnboardingApp.Views.RootView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:OnboardingApp.ViewModels"
        xmlns:s="https://github.com/canton7/Stylet"
        xmlns:local="clr-namespace:OnboardingApp"
        d:DataContext="{d:DesignInstance Type=viewModels:RootViewModel}"
        mc:Ignorable="d"
        Title="RootView" Height="450" Width="800">
    <Grid>
        <ContentControl Margin="5,5,5,5" s:View.Model="{Binding ActiveItem}" />
    </Grid>
</hc:Window>
